#Grammar
<dec_program> ::= pr_begin <lista_instrucoes> pr_end ;

<lista_instrucoes> ::= <instrucao> ";" <resto_lista> ;
<resto_lista> ::=  î | <lista_instrucoes> ;

<instrucao> ::= <dec_var> | <comando>;
<dec_var> ::= <tipo> <lista_identificadores>;
<tipo> ::= pr_tipoInt | pr_tipoFloat | pr_tipoString | pr_tipoBoolean | <Lista> ;
<Lista> ::= pr_list "(" <tipoSimples> "," <tamanhoLista> ")";
<tipoSimples> ::= pr_tipoInt | pr_tipoFloat | pr_tipoString | pr_tipoBoolean ;
<tamanhoLista> ::= Const_int ;

<lista_identificadores> ::= <identificador> <resto_lista2> ;
<resto_lista2> ::= î | "," <lista_identificadores> ;

<identificador> ::= id;

<comando> ::= id <comando_id> | <entrada_dados> |<saida_dados> | <comando_selecao> | <comando_repeticao> ;
<comando_id> ::= <operador_atribuicao> <expressao> | <tipo_operacao_lista>;

<operador_atribuicao> ::= "=" | "<-" ;

<tipo_operacao_lista> ::= pr_add "("<elemento>","<posicao>")" | pr_delete "("<posicao>")" ;


<elemento> ::= <expressao> ;
<posicao> ::= <expressao> ;

<entrada_dados> ::= pr_read "("<lista_entradas>")" ;

<lista_entradas> ::= <entrada_simples> | <entrada_complexa> ;
<entrada_simples> ::= id <resto_lista3> ;
<resto_lista3> ::=  î | "," <lista_entradas> ;
<entrada_complexa> ::= Const_string "," id <resto_lista3> ;

<saida_dados> ::= pr_print "(" <lista_expressoes>")" ;

<lista_expressoes> ::= <expressao> <resto_lista_expressoes> ;
<resto_lista_expressoes> ::=  î | "," <lista_expressoes> ;

<comando_selecao> ::= pr_if <expressao> <lista_comandos> <parte_else> ;
<parte_else> ::= pr_else <lista_comandos> pr_end | pr_end ;

<lista_comandos> ::= <comando> ";" <resto_lista_comandos> ;
<resto_lista_comandos> ::=  î | <lista_comandos> ;

<comando_repeticao> ::= pr_do <lista_comandos> pr_until <expressao> ;


<expressao> ::= <valor> <expressao_> ;
<expressao_> ::=  î | pr_and <valor> <expressao_> | pr_or <valor> <expressao_> ;
<valor> ::= <relacional> | pr_true | pr_false | pr_not <valor> ;
<relacional> ::= <aritmetica> <relacional_> ;
<relacional_> ::=  î | <operador_relacional> <aritmetica> ;
<operador_relacional>::= "==" | "<" | ">" ;
<aritmetica> ::= <termo> <aritmetica_> ;
<aritmetica_> ::=  î | "+" <termo> <aritmetica_> | "-" <termo> <aritmetica_> ;
<termo> ::= <fator> <termo_> ;
<termo_> ::=  î | "*" <fator> <termo_> | "/" <fator> <termo_> ;
<fator> ::= id <fator_> | Const_int |Const_float | Const_string | "(" <expressao> ")" | "+" <fator> |"-" <fator> ;
<fator_> ::=  î | pr_count | pr_size | pr_elementOf "(" <expressao> ")" ;

