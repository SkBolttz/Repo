#Options
GenerateScanner = true
GenerateParser = true
Language = Java
ScannerName = Lexico
ParserName = Sintatico
SemanticName = Semantico
ScannerCaseSensitive = true
ScannerTable = Compact
Input = Stream
Parser = LL
#RegularDefinitions
dig:[0-9]
mai:[A-Z]
min:[a-z]
#Tokens
id: {min} ({mai} {min} | {min})* ("_"{dig}+)? | {mai} {min}+ ({mai} {min}+)* ("_"{dig}+)?
 "+"
Const_int:([1-9]{dig}?{dig}?{dig}?{dig}? | 0)
Const_float:([1-9]{dig}?{dig}?{dig}?{dig}? | 0) "." (0 | {dig}?{dig}?{dig}?{dig}?[1-9])
Const_string:\"[^\n\""\"]*\"
Comentario_linha : %[^\n]*
Comentario_bloco: "(*"[^\(\*\)]*"*)"

pr_tipoInt = id: "int"
pr_tipoFloat = id: "float"
pr_tipoString = id: "string"
pr_tipoBoolean = id: "bool"
pr_list = id: "list"
pr_add = id: "add"
pr_delete = id: "delete"
pr_read = id: "read"
pr_print = id: "print"
pr_if = id: "if"
pr_else = id: "else"
pr_end = id: "end"
pr_do = id: "do"
pr_until = id: "until"
pr_begin = id: "begin"
pr_and = id: "and"
pr_or = id: "or"
pr_not = id: "not"
pr_count = id: "count"
pr_size = id: "size"
pr_elementOf = id: "elementOf"
pr_true = id: "true"
pr_false = id: "false"

"<-"
